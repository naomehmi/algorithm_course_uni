angka = input().split()
isprima = 0
for i in range(len(angka)):
    if(int(angka[i]) == 2):
        isprima += 1
    elif(int(angka[i]) == 1):
         continue
    else:
        temp = "n"
        for j in range(2,int(angka[i])):
            if(int(angka[i]) % j == 0):
                temp = "y"
                break
        if(temp != "y"):
            isprima += 1
print(isprima)
#COMPLEXITY = O(n^2)
